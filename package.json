{
  "name": "vite-reactts-electron-starter",
  "version": "0.5.0",
  "license": "MIT",
  "main": "main/index.js",
  "author": {
    "name": "Maximilian St√ºmpfl"
  },
  "description": "Starter using Vite 2+ React +Typescript + Electron for fast prototyping.",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/lTimeless/vite-reactts-electron-starter"
  },
  "productName": "vite-reactts-electron-starter",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \" npm run dev:electron\" \"npm run server \"",
    "dev:vite": "vite",
    "server": "json-server --watch bd_full.json -p 8804",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/raleway": "^4.5.12",
    "@mui/material": "^5.11.14",
    "@mui/system": "^5.11.15",
    "@types/axios": "^0.14.0",
    "@types/qs": "^6.9.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^1.2.0",
    "axios": "^1.3.5",
    "chart.js": "^4.3.0",
    "electron-is-dev": "^2.0.0",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-router-dom": "^6.9.0",
    "sass": "^1.59.3",
    "vite": "^2.8.6"
  },
  "devDependencies": {
    "@types/node": "^17.0.22",
    "@types/react": "^17.0.58",
    "@types/react-dom": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "electron": "^23.1.4",
    "electron-builder": "^22.14.13",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "json-server": "^0.17.3",
    "postcss": "^8.4.12",
    "prettier": "2.6.0",
    "typescript": "^4.6.2"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
